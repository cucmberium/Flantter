            <Grid Margin="0,12">
                <Grid.RowDefinitions>
                    <RowDefinition Height="Auto"/>
                    <RowDefinition Height="Auto"/>
                </Grid.RowDefinitions>

                <StackPanel Grid.Row="1">
                    <Border Background="#FFB01E00" CornerRadius="8" HorizontalAlignment="Center" VerticalAlignment="Center" Width="100" Height="100">
                        <Image Source="ms-appx:///Assets/Square150x150Logo.scale-200.png" Width="100" Height="100"/>
